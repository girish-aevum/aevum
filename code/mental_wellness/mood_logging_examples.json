{
  "mood_logging_workflow": {
    "description": "Complete workflow for mood logging and tracking",
    "steps": [
      {
        "step": 1,
        "action": "Get Reference Data",
        "endpoints": [
          "GET /api/mental-wellness/emotions/",
          "GET /api/mental-wellness/activities/",
          "GET /api/mental-wellness/triggers/",
          "GET /api/mental-wellness/mood-categories/"
        ],
        "description": "Fetch available emotions, activities, triggers, and mood categories"
      },
      {
        "step": 2,
        "action": "Create Mood Entry",
        "endpoint": "POST /api/mental-wellness/mood-entries/create/",
        "description": "Log a detailed mood entry",
        "payload": {
          "entry_date": "2023-12-10",
          "entry_time": "14:30:00",
          "mood_level": 7,
          "energy_level": 8,
          "anxiety_level": 3,
          "stress_level": 4,
          "sleep_quality": 6,
          "sleep_hours": 7.5,
          "mood_category": 1,
          "emotion_ids": [1, 5, 8],
          "activity_ids": [2, 15, 23],
          "trigger_ids": [7],
          "notes": "Had a great workout this morning, feeling energized!",
          "gratitude_note": "Grateful for good health and supportive friends",
          "goals_tomorrow": "Continue with morning exercise routine",
          "weather_condition": "Sunny",
          "location": "Home"
        }
      },
      {
        "step": 3,
        "action": "Quick Mood Entry",
        "endpoint": "POST /api/mental-wellness/quick-mood/",
        "description": "Log a quick mood entry with minimal data",
        "payload": {
          "mood_level": 6,
          "energy_level": 5,
          "anxiety_level": 4,
          "notes": "Feeling okay today"
        }
      },
      {
        "step": 4,
        "action": "View Dashboard",
        "endpoint": "GET /api/mental-wellness/dashboard/",
        "description": "Get comprehensive dashboard data"
      },
      {
        "step": 5,
        "action": "View Statistics",
        "endpoint": "GET /api/mental-wellness/statistics/",
        "description": "Get detailed mood analytics and trends"
      }
    ]
  },
  "api_endpoints": {
    "reference_data": [
      {
        "method": "GET",
        "url": "/api/mental-wellness/mood-categories/",
        "description": "Get all mood categories",
        "authentication": "Not required",
        "response_example": [
          {
            "id": 1,
            "name": "Joyful",
            "description": "Happy, elated, cheerful moods",
            "color_hex": "#4CAF50",
            "icon": "ðŸ˜Š",
            "is_active": true
          }
        ]
      },
      {
        "method": "GET",
        "url": "/api/mental-wellness/emotions/",
        "description": "Get all emotions",
        "authentication": "Not required",
        "query_parameters": {
          "type": "POSITIVE, NEGATIVE, NEUTRAL, MIXED - Filter by emotion type",
          "search": "Search in emotion names and descriptions"
        },
        "response_example": [
          {
            "id": 1,
            "name": "Happy",
            "emotion_type": "POSITIVE",
            "description": null,
            "intensity_scale": "1-10",
            "color_hex": "#4CAF50",
            "is_active": true
          }
        ]
      },
      {
        "method": "GET",
        "url": "/api/mental-wellness/activities/",
        "description": "Get all activity types",
        "authentication": "Not required",
        "query_parameters": {
          "category": "EXERCISE, SOCIAL, WORK, etc. - Filter by category",
          "search": "Search in activity names and descriptions"
        }
      },
      {
        "method": "GET",
        "url": "/api/mental-wellness/triggers/",
        "description": "Get all mood triggers",
        "authentication": "Not required",
        "query_parameters": {
          "category": "STRESS, RELATIONSHIPS, WORK, etc. - Filter by category",
          "search": "Search in trigger names and descriptions"
        }
      }
    ],
    "mood_entries": [
      {
        "method": "POST",
        "url": "/api/mental-wellness/mood-entries/create/",
        "description": "Create detailed mood entry",
        "authentication": "Required",
        "required_fields": ["mood_level", "energy_level", "anxiety_level"],
        "optional_fields": [
          "entry_date", "entry_time", "stress_level", "sleep_quality", 
          "sleep_hours", "mood_category", "emotion_ids", "activity_ids", 
          "trigger_ids", "notes", "gratitude_note", "goals_tomorrow", 
          "weather_condition", "location"
        ]
      },
      {
        "method": "POST",
        "url": "/api/mental-wellness/quick-mood/",
        "description": "Create quick mood entry",
        "authentication": "Required",
        "required_fields": ["mood_level", "energy_level", "anxiety_level"],
        "optional_fields": ["notes"]
      },
      {
        "method": "GET",
        "url": "/api/mental-wellness/mood-entries/",
        "description": "List user's mood entries",
        "authentication": "Required",
        "query_parameters": {
          "date_from": "Filter entries from this date (YYYY-MM-DD)",
          "date_to": "Filter entries to this date (YYYY-MM-DD)",
          "mood_min": "Minimum mood level (1-10)",
          "mood_max": "Maximum mood level (1-10)",
          "filter": "today, week, month - Quick date filters",
          "search": "Search in notes and gratitude notes",
          "ordering": "entry_date, -entry_date, mood_level, etc."
        }
      },
      {
        "method": "GET",
        "url": "/api/mental-wellness/mood-entries/{id}/",
        "description": "Get detailed mood entry",
        "authentication": "Required"
      },
      {
        "method": "PUT/PATCH",
        "url": "/api/mental-wellness/mood-entries/{id}/update/",
        "description": "Update existing mood entry",
        "authentication": "Required"
      },
      {
        "method": "DELETE",
        "url": "/api/mental-wellness/mood-entries/{id}/delete/",
        "description": "Delete mood entry",
        "authentication": "Required"
      }
    ],
    "analytics": [
      {
        "method": "GET",
        "url": "/api/mental-wellness/dashboard/",
        "description": "Get comprehensive dashboard data",
        "authentication": "Required",
        "response_includes": [
          "Today's entry status",
          "Current logging streak",
          "Week's average mood",
          "Mood trend",
          "Recent entries",
          "Active insights",
          "Suggested activities"
        ]
      },
      {
        "method": "GET",
        "url": "/api/mental-wellness/statistics/",
        "description": "Get detailed mood statistics",
        "authentication": "Required",
        "response_includes": [
          "Entry counts (total, week, month)",
          "Average scores (mood, energy, anxiety, wellbeing)",
          "Mood trends",
          "Logging streak",
          "Most common emotions/activities/triggers",
          "Recent entries and insights"
        ]
      }
    ],
    "insights": [
      {
        "method": "GET",
        "url": "/api/mental-wellness/insights/",
        "description": "Get AI-generated mood insights",
        "authentication": "Required",
        "query_parameters": {
          "type": "PATTERN, CORRELATION, TREND, etc. - Filter by insight type",
          "acknowledged": "true/false - Filter by acknowledgment status"
        }
      },
      {
        "method": "POST",
        "url": "/api/mental-wellness/insights/{id}/acknowledge/",
        "description": "Mark insight as acknowledged",
        "authentication": "Required"
      }
    ]
  },
  "sample_payloads": {
