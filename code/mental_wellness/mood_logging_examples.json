  },
  "mood_scales": {
    "mood_level": {
      "scale": "1-10",
      "descriptions": {
        "1": "Very Low - Extremely down, depressed",
        "2": "Low - Feeling sad, blue",
        "3": "Below Average - Somewhat down",
        "4": "Average - Neither good nor bad",
        "5": "Above Average - Slightly positive",
        "6": "Good - Generally positive mood",
        "7": "Very Good - Happy, upbeat",
        "8": "Great - Very happy, joyful",
        "9": "Excellent - Extremely happy",
        "10": "Outstanding - Euphoric, on top of the world"
      }
    },
    "energy_level": {
      "scale": "1-10",
      "descriptions": {
        "1": "Exhausted - No energy at all",
        "2": "Very Low - Barely functioning",
        "3": "Low - Sluggish, tired",
        "4": "Below Average - Somewhat tired",
        "5": "Average - Normal energy levels",
        "6": "Above Average - Good energy",
        "7": "Good - Feeling energetic",
        "8": "High - Very energetic",
        "9": "Very High - Extremely energetic",
        "10": "Energetic - Boundless energy"
      }
    },
    "anxiety_level": {
      "scale": "1-10",
      "descriptions": {
        "1": "No Anxiety - Completely calm",
        "2": "Very Low - Minimal worry",
        "3": "Low - Slight nervousness",
        "4": "Mild - Some worry or tension",
        "5": "Moderate - Noticeable anxiety",
        "6": "Above Average - Increased worry",
        "7": "High - Significant anxiety",
        "8": "Very High - Intense worry",
        "9": "Severe - Overwhelming anxiety",
        "10": "Extreme - Panic level anxiety"
      }
    }
  },
  "validation_rules": {
    "mood_entry": {
      "required_fields": ["mood_level", "energy_level", "anxiety_level"],
      "field_constraints": {
        "mood_level": "Integer 1-10",
        "energy_level": "Integer 1-10", 
        "anxiety_level": "Integer 1-10",
        "stress_level": "Integer 1-10 (optional)",
        "sleep_quality": "Integer 1-10 (optional)",
        "sleep_hours": "Decimal 0.0-24.0 (optional)",
        "entry_date": "Date, cannot be in future",
        "entry_time": "Time, cannot be in future for today's date",
        "emotion_ids": "Array of valid emotion IDs",
        "activity_ids": "Array of valid activity type IDs",
        "trigger_ids": "Array of valid mood trigger IDs"
      },
      "validation_errors": [
        "Entry date cannot be in the future",
        "Entry time cannot be in the future for today's date",
        "A mood entry already exists for this date and time",
        "Invalid emotion IDs: [list]",
        "Invalid activity IDs: [list]",
        "Invalid trigger IDs: [list]"
      ]
    }
  },
