    "get_statistics": {
      "command": "curl -X GET http://localhost:8000/api/mental-wellness/statistics/ -H 'Authorization: Bearer YOUR_JWT_TOKEN'",
      "description": "Get mood statistics"
    },
    "list_entries": {
      "command": "curl -X GET 'http://localhost:8000/api/mental-wellness/mood-entries/?filter=week' -H 'Authorization: Bearer YOUR_JWT_TOKEN'",
      "description": "List this week's mood entries"
    }
  },
  "integration_notes": {
    "ai_insights": "System automatically generates insights after 7, 14, 30 days and every 30 days thereafter",
    "wellbeing_score": "Calculated using weighted average: mood (40%), energy (30%), inverted anxiety (20%), inverted stress (10%)",
    "trend_calculation": "Compares recent 7-day average with previous 7-day average",
    "streak_tracking": "Counts consecutive days with at least one mood entry",
    "data_privacy": "All mood data is user-specific and access-controlled",
    "scalability": "Pagination implemented for large datasets",
    "extensibility": "Easy to add new emotions, activities, and triggers via admin interface"
  }
} 