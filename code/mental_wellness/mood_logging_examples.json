  "curl_examples": {
    "create_mood_entry": {
      "command": "curl -X POST http://localhost:8000/api/mental-wellness/mood-entries/create/ -H 'Authorization: Bearer YOUR_JWT_TOKEN' -H 'Content-Type: application/json' -d '{\"mood_level\": 7, \"energy_level\": 8, \"anxiety_level\": 3, \"notes\": \"Feeling great today!\"}'",
      "description": "Create a mood entry"
    },
    "quick_mood": {
      "command": "curl -X POST http://localhost:8000/api/mental-wellness/quick-mood/ -H 'Authorization: Bearer YOUR_JWT_TOKEN' -H 'Content-Type: application/json' -d '{\"mood_level\": 6, \"energy_level\": 5, \"anxiety_level\": 4}'",
      "description": "Quick mood logging"
    },
    "get_dashboard": {
      "command": "curl -X GET http://localhost:8000/api/mental-wellness/dashboard/ -H 'Authorization: Bearer YOUR_JWT_TOKEN'",
      "description": "Get dashboard data"
