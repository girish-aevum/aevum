  "field_requirements": {
    "required_fields": {
      "street": {
        "description": "Street address including apartment/unit number",
        "max_length": 200,
        "examples": [
          "123 Main Street",
          "456 Oak Ave, Apt 4B",
          "789 Business Blvd, Suite 100"
        ]
      },
      "city": {
        "description": "City name",
        "max_length": 100,
        "examples": ["San Francisco", "Toronto", "Austin"]
      },
      "state": {
        "description": "State, province, or region",
        "max_length": 100,
        "examples": ["CA", "California", "Ontario", "TX"]
      },
      "zip_code": {
        "description": "ZIP code, postal code, or equivalent",
        "max_length": 20,
        "examples": ["94105", "M5V 2A9", "73301", "10001"]
      },
      "country": {
        "description": "Country name",
        "max_length": 100,
        "examples": ["USA", "Canada", "United States", "United Kingdom"]
      }
    },
    "optional_fields": {
      "quantity": {
        "description": "Number of kits to order",
        "default": 1,
        "type": "positive_integer"
      },
      "shipping_method": {
        "description": "Shipping speed preference",
        "default": "STANDARD",
        "choices": ["STANDARD", "EXPRESS", "OVERNIGHT"],
        "costs": {
          "STANDARD": "$0.00",
          "EXPRESS": "$15.00", 
          "OVERNIGHT": "$35.00"
        }
      },
      "special_instructions": {
        "description": "Delivery instructions for the carrier",
        "max_length": 500,
        "examples": [
          "Leave at front door",
          "Deliver to reception desk",
          "Ring doorbell twice"
        ]
      }
    }
  }
